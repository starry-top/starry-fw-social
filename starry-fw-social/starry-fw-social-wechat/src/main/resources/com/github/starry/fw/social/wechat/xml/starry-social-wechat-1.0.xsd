<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:wechat="http://github.com/starry-top/starry-fw-social/schema/social/wechat"
    targetNamespace="http://github.com/starry-top/starry-fw-social/schema/social/wechat">

    <xs:element name="config">
        <xs:complexType>
            <xs:attribute name="app-id" use="required" type="xs:string" />
            <xs:attribute name="app-secret" use="required" type="xs:string" />
            <xs:attribute name="app-namespace" use="optional" type="xs:string" />
        </xs:complexType>
    </xs:element>

</xs:schema>